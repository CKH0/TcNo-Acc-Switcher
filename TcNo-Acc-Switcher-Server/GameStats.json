{
  "StatsDefinitions": {
    "Apex Legends": {
      "Indicator": "A",
      "Url": "https://apexlegendsstatus.com/core/interface?platform=PC&player={Username}",
      "Vars": {
        "Username": "Origin Username"
      },
      "Collect": {
        "LVL": {
          "XPath": "\"//*[@class='levelNumber']\"",
          "Select": "innerHtml",
          "DisplayAs": "LVL %x%"
        },
        "BR": {
          "XPath": "\"//*[contains(text(), ' RP')]\"",
          "Select": "innerText",
          "DisplayAs": "%x%",
          "NoDisplayIf": "0 RP"
        },
        "AR": {
          "XPath": "\"//*[contains(text(), ' AR')]\"",
          "Select": "innerText",
          "DisplayAs": "%x%",
          "NoDisplayIf": "0 AP"
        }
      }
    }
  },
  "PlatformCompatibilities": {
    "Origin": [ "Apex Legends" ],
    "Steam": [ "Apex Legends" ]
  }
}